<%- include("../header"); %>
<div class="col-12">
    <h1>Agregar usuario</h1>
    <form method="post" action="/personas/insertar">
        <div class="form-group">
            <label for="nombre">Nombre</label>
            <input required id="nombre" placeholder="Nombre del usuario" class="form-control" type="text"
                name="nombre">
        </div>
        <div class="form-group">
            <label for="precio">apellido</label>
            <input required id="precio" placeholder="Precio del producto" class="form-control" type="text"
                name="apellido">
        </div>
        <div class="form-group">
            <button class="btn btn-success">Guardar</button>
            &nbsp;<a href="/personas" class="btn btn-primary">Volver</a>
        </div>
    </form>
</div>
<%- include("../footer"); %>